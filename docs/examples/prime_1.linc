function void main()
{
	int i=2
	int n
	print("Enter max.: ")
	input_int(n)
	print("The prime numbers are:".endl)
	while((i<n))
	{
		int flag=1
		int j=2
		while((j<i))
		{
			if((i%j==0))
			{
				flag=0
			}
			j=j+1
		}
		if((flag==1))
		{
			print(i.endl)
		}
		i=i+1
	}
	terminate()
}